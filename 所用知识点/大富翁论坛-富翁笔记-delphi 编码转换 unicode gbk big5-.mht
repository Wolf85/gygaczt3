function UnicodeEncode(Str:string;CodePage:integer):WideString;
var
  Len:integer;
begin
  Len:=3DLength(Str)+1;
  SetLength(Result,Len);
  =
Len:=3DMultiByteToWideChar(CodePage,0,PChar(Str),-1,PWideChar(Result),Len=
);
  SetLength(Result,Len-1); //end is #0
end;

function UnicodeDecode(Str:WideString;CodePage:integer):string;
var
  Len:integer;
begin
  Len:=3DLength(Str)*2+1;  //one for #0
  SetLength(Result,Len);
  =
Len:=3DWideCharToMultiByte(CodePage,0,PWideChar(Str),-1,PChar(Result),Len=
,nil,nil);
  SetLength(Result,Len-1);
end;

function Gb2Big5(Str:string):string;
begin
  SetLength(Result,Length(Str));
  LCMapString(GetUserDefaultLCID,LCMAP_TRADITIONAL_CHINESE,
    PChar(Str),Length(Str),
    PChar(Result),Length(Result));
  Result:=3DUnicodeDecode(UnicodeEncode(Result,936),950);
end;

function Big52Gb(Str:string):string;
begin
  Str:=3DUnicodeDecode(UnicodeEncode(Str,950),936);
  SetLength(Result,Length(Str));
  LCMapString(GetUserDefaultLCID,LCMAP_SIMPLIFIED_CHINESE,
    PChar(Str),Length(Str),
    PChar(Result),Length(Result));
end;

=B9=D8=BC=FC=CA=B9=D3=C3=C1=CBUnicodeToUtf8=D5=E2=B8=F6=BA=AF=CA=FD
function Utf8Encode(const WS: WideString): UTF8String;
var
  L: Integer;
  Temp: UTF8String;
begin
  Result :=3D '';
  if WS =3D '' then Exit;
  SetLength(Temp, Length(WS) * 3); // SetLength includes space for null =
terminator
  L :=3D UnicodeToUtf8(PChar(Temp), Length(Temp)+1, PWideChar(WS), =
Length(WS));
  if L &gt; 0 then
    SetLength(Temp, L-1)
  else
    Temp :=3D '';
  Result :=3D Temp;
end;</PRE><BR>
